  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "release": "release-it",
    "copy:components": "mkdir -p lib && cp -R .tmp/@opensumi/gatsby-theme/site/components lib && cp -R .tmp/@opensumi/gatsby-theme/site/*.* lib && cp site/components/*.less ./lib/components && cp site/common.json ./lib && cp -R site/images ./lib",
    "compile:components": "rm -rf .tmp lib && ../../node_modules/.bin/tsc --outDir .tmp --noEmit false -d && npm run copy:components",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opensumi/gatsby-theme"
  },
  "bugs": {
    "url": "https://github.com/opensumi/gatsby-theme/issues"
  },
